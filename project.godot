[physics]

3d/smooth_trimesh_collision=true
common/enable_pause_aware_picking=true
3d/physics_engine="Bullet"
3d/default_gravity=20.0

[qodot]

textures/normal_pattern="%s_Normal.%s"
textures/metallic_pattern="%s_Metallic.%s"
textures/roughness_pattern="%s_Roughness.%s"
textures/emission_pattern="%s_Emission.%s"
textures/ao_pattern="%s_AO.%s"
textures/depth_pattern="%s_Depth.%s"

[rendering]

2d/options/use_nvidia_rect_flicker_workaround=true
threads/thread_model=2
threads/thread_safe_bvh=true
vram_compression/import_etc=true
misc/lossless_compression/force_png=true
quality/shadows/filter_mode=0
quality/reflections/texture_array_reflections=false
quality/reflections/high_quality_ggx=false
quality/shading/force_vertex_shading.mobile=false
quality/shading/force_lambert_over_burley=true
quality/shading/use_physical_light_attenuation=true
quality/filters/anisotropic_filter_level=8
2d/options/use_software_skinning=false
2d/options/ninepatch_mode=0
misc/occlusion_culling/max_active_polygons=16
gles3/shaders/shader_cache_size_mb=4096
quality/spatial_partitioning/bvh_collision_margin=0.0
quality/subsurface_scattering/quality=0
environment/default_environment="res://default_env.tres"
